@{
    ViewData["Title"] = "Lich Thi Dau Page";
    Layout = "~/Views/Shared/_Layout_LichThiDau.cshtml";
}
<div id="cmsmasters_row_6406e5d4e72746_30356617"
     class="cmsmasters_row cmsmasters_color_scheme_default">
    <div class="cmsmasters_row_outer_parent">
        <div class="cmsmasters_row_outer">
            <div class="cmsmasters_row_inner">
                <div class="cmsmasters_row_margin">
                    <div class="cmsmasters_column one_first">
                        <div id="cmsmasters_heading_6406e5d4e73871_97325584"
                             class="cmsmasters_heading_wrap cmsmasters_heading_align_left">
                            <h1 class="cmsmasters_heading">LỊCH THI ĐẤU</h1>
                        </div>
                        <div id="cmsmasters_divider_6406e5d4e73a70_57344397"
                             class="cmsmasters_divider cmsmasters_divider_width_short cmsmasters_divider_pos_left"></div>
                        <div class="cmsmasters_wrap_table">
                            <table class="cmsmasters_table">
                                <caption></caption>
                                <thead>
                                    <tr class="cmsmasters_table_row_header">
                                        <th class="cmsmasters_table_cell_alignleft">
                                            TRẬN ĐẤU
                                        </th>
                                        <th class="cmsmasters_table_cell_alignright"></th>
                                        <th class="cmsmasters_table_cell_aligncenter"></th>
                                        <th class="cmsmasters_table_cell_alignleft"></th>
                                        <th class="cmsmasters_table_cell_alignright">
                                            MUA VÉ
                                        </th>
                                    </tr>
                                </thead>
                                @for (var i = 0; i < Model.Count; i++)
                                {
                                    <tr>
                                        <td class="cmsmasters_table_cell_alignleft">
                                            @Model[i].StartTime
                                        </td>
                                        <td class="cmsmasters_table_cell_alignright">
                                            @Model[i].TenClbNha
                                        </td>
                                        <td class="cmsmasters_table_cell_aligncenter">
                                            VS
                                        </td>
                                        <td class="cmsmasters_table_cell_alignleft">
                                            @Model[i].TenClbKhach
                                        </td>
                                        <td class="cmsmasters_table_cell_alignright">
                                            <a href="#">Mua vé</a>
                                        </td>
                                    </tr>
                                }
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>